{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "()": "config.logging_config.JSONFormatter"
    }
  },
  "handlers": {
    "default": {
      "formatter": "json",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    },
    "access": {
      "formatter": "json",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["access"],
      "level": "INFO",
      "propagate": false
    }
  }
}

